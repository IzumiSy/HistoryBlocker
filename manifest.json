{
	"name": "History Blocker",
	"version": "1.0",
	"manifest_version": 2,
	
	"description": "Blocks some specific pages from recording to browser's history",
	
	"permissions": [
    "browsingData"
  ],
	
	"options_page": "options.html",
	
	"background": {
		"scripts": ["background.js"]
	},
	
	"browser_action": {
		"default_icon": "main.png",
		"default_title": "History Blocker"
	},
	
	"icons": { "128": "main.png" }	
}